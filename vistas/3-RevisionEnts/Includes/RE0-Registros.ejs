<% if (regs && regs.length) { %>
	<h3><%= titulo %> (<%= regs.length %>)</h3>
	<% for (let reg of regs) { %>
		<a href="/<%= url %>/?entidad=<%= reg.entidad %>&id=<%= reg.id + (reg.edicID ? ('&edicID=' + reg.edicID) : '') + '&origen=' + (locals.origen ? origen : 'TE') %>">
			<p><%= reg.fechaRefTexto %> - <%= reg.abrev %> - <%= reg.nombre %></p>
		</a>
	<% } %>	
<% } %>