<!-- General -->
<link rel="stylesheet" href="/formatos/3-RevisionEnts/RE0-Revision.css">

<!-- TABLERO -->
<% if (codigo == "tableroControl") { %>
	<link rel="stylesheet" href="/formatos/0-Compartido/Main/TableroControl.css">

<% } else { %>
	<!-- Prod-Altas -->
	<% if (codigo == "producto/alta") { %>
		<link rel="stylesheet" href="/formatos/2.1-Prods-CRUD/PR-1Detalle.css">
		<link rel="stylesheet" href="/formatos/3-RevisionEnts/RE1-Prod-AltaEdicion.css">
		<link rel="stylesheet" href="/formatos/0-Compartido/Main/DatosBreves.css">

	<!-- RCLV-Altas -->
	<% } else if (codigo == "rclv/alta") { %>
		<link rel="stylesheet" href="/formatos/2.2-RCLVs-CRUD/RCLV-AltaEdicion.css">
		<script src="/javascript/2.2-RCLVs-CRUD/RCLV-InputError.js"></script>

	<!-- Prod-Avatar -->
	<% } else if (codigo == "producto/edicion/avatar") { %>
		<link rel="stylesheet" href="/formatos/3-RevisionEnts/RE1-Prod-Avatar.css">
		<script src="/javascript/3-RevisionEnts/RE1-Prod-Avatar.js"></script>
	
	<!-- Ediciones -->
	<% } else if (codigo == "producto/edicion" || codigo == "rclvEdicion") { %>
		<link rel="stylesheet" href="/formatos/0-Compartido/Main/DatosBreves.css">
		<link rel="stylesheet" href="/formatos/3-RevisionEnts/RE9-Edicion.css">
		<script src="/javascript/3-RevisionEnts/RE9-Edicion.js"></script>
		<!-- Específico para productos -->
		<link rel="stylesheet" href="/formatos/3-RevisionEnts/RE1-Prod-AltaEdicion.css">
	
	<!-- Links -->
	<% } else if (codigo == "abmLinks") { %>
		<!-- Uso compartido -->
		<link rel="stylesheet" href="/formatos/2.3-Links-CRUD/Links.css">
		<script src="/javascript/2.3-Links-CRUD/LK0-ActivosInactivos.js"></script>
		<script src="/javascript/2.3-Links-CRUD/LK0-PeliTrailer.js"></script>
		<script src="/javascript/2.3-Links-CRUD/LK2-Eliminar.js"></script>
		<!-- Específico -->
		<script src="/javascript/3-RevisionEnts/RE3-Link1-Altas.js"></script>
		<script src="/javascript/3-RevisionEnts/RE3-Link2-Ediciones.js"></script>
	<% } %>
<% } %>
