<link rel="stylesheet" href="/formatos/3-RevisionEnts/0-Revision.css">
<%- include("CMP-RV-Head", {codigo}) %>
<% if (codigo != "tableroControl") { %>
	<!-- Específicos para Prod-Altas -->
	<% if (codigo == "producto/alta") { %>
		<link rel="stylesheet" href="/formatos/3-RevisionEnts/1-Prod-Alta.css">
		<script src="/javascript/3-RevisionEnts/1-Prod1_Alta.js"></script>
	<!-- Específicos para Prod-Edicion -->
	<% } else if (codigo == "producto/edicion") { %>
		<% if (vista.includes("Avatar")) { %>
			<link rel="stylesheet" href="/formatos/3-RevisionEnts/1-Prod-EdicAvatar.css">
			<script src="/javascript/3-RevisionEnts/1-Prod2_Avatar.js"></script>
		<% } else { %>
			<link rel="stylesheet" href="/formatos/3-RevisionEnts/1-Prod-EdicDemas.css">
			<script src="/javascript/3-RevisionEnts/1-Prod3_Edicion.js"></script>
		<% } %>
	<!-- Específicos para RCLV-Altas -->
	<% } else if (codigo == "rclv/alta") { %>
		<link rel="stylesheet" href="/formatos/2.0-CRUD/2-RCLV-AltaEdicion.css">
		<script src="/javascript/2.0-CRUD/2-RCLV.js"></script>
	<!-- Específicos para Links -->
	<% } else if (codigo == "links") { %>
		<!-- Uso compartido -->
		<link rel="stylesheet" href="/formatos/0-Compartido/Main/Links.css">
		<script src="/javascript/0-Compartido/LK-PeliTrailer.js"></script>
		<script src="/javascript/0-Compartido/LK-ActivosInactivos.js"></script>
		<script src="/javascript/0-Compartido/LK-Eliminar.js"></script>
		<!-- Específico -->
		<link rel="stylesheet" href="/formatos/3-RevisionEnts/3-Links.css">
		<script src="/javascript/3-RevisionEnts/3-Links1-Altas.js"></script>
		<script src="/javascript/3-RevisionEnts/3-Links2-Ediciones.js"></script>
	<% } %>
<% } %>