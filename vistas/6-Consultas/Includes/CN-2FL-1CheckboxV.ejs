<div id="<%= tema.codigo %>" class="filtro checkbox flexColAlign">
	<h3><%= tema.titulo %> </h3>
	<div id="label-inputs" class="flexRowAlign">
		<% for (let opcion of tema.opciones) { %>
			<div class="label-input flexRowAlign">
				<% let nombre = opcion.nombre %>
				<% if (nombre.length > 5) nombre = opcion.nombre.slice(0, 5)+"." %> 
				<label for="<%= opcion.id %>"><%= nombre %></label>
				<input 
					type="checkbox" name="<%= tema.codigo %>" value="<%= opcion.id %>" 
					id="<%= opcion.id %>" class="elegibleComplejo" 
					<% if (opcionesElegidas[tema.codigo]&&opcionesElegidas[tema.codigo].includes(opcion.id)) { %>checked<% } %>
				>
			</div>
		<% } %>
	</div>
</div>
