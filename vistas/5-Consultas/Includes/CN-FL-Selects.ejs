<div id="<%= asunto.codigo %>" class="pref contSelect <%- locals.color ? color : '' %>">
	<select name="<%= asunto.codigo %>" class="<%= locals.eventual ? asunto.codigo : 'presenciaEstable' %> prefSimple">
		<option value=""><%= asunto.titulo %></option>
		<% for (let opcion of asunto.opciones) { %>
			<option value="<%= opcion.id %>" class="<%= opcion.clase %>"><%= opcion.nombre %></option>
		<% } %>
	</select>
</div>
