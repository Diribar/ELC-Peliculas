<section class="label-input">
	<% let inputOK = dataEntry[registro.nombreDelCampo] && !errores[registro.nombreDelCampo] %>
	<label><%= registro.titulo %></label>
	<!-- Data-entry y Errores -->
	<div class="inputError">
		<input type="text" class="input<% if (inputOK) { %> despl_click<% } %>" name="<%= registro.nombreDelCampo %>" autocomplete="off"
			value="<%= dataEntry[registro.nombreDelCampo] %>"
			<% if (registro.numero) { %>id="numero" placeholder="sólo números"<% } %>
			<% if (inputOK) { %>disabled<% } else { %>autofocus<% } %>
		>
		<!-- Ayuda -->
		<%- include("PA3-DD9-Mensajes") %>
		<!-- Errores -->
		<i class="fa-solid fa-circle-xmark ocultar"><span class="mensajeError"></span></i>
	</div>
</section>
