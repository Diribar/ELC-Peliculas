<% const campo = "tipo_actuacion_id" %>
<% let {valores} = camposDA.find(n => n.nombre == campo) %>
<div id="tipoActuacion" class="inputError flexRowAlign altoFila">
	<% for (let valor of valores) { %>
		<div class="flexRowAlign">
			<input id="<%= valor.id %>" type="radio" name="tipo_actuacion_id" value="<%= valor.id %>" <% if (!camposDE.includes(campo)&&valor.id=="1" || dataEntry.tipo_actuacion_id==valor.id) { %>checked<% } %> class="tipoActuacion">
			<label for="<%= valor.id %>"><%= valor.nombre %></label>
		</div>
	<% } %>
	<%- include("CMP-Errores", {display: true, campo}) %>
</div>
