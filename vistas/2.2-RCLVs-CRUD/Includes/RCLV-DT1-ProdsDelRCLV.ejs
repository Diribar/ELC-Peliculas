<% for (let prod of prodsDelRCLV) { %>
	<li id="prodsNuevos">
		<a href="/producto/detalle/?entidad=<%= prod.entidad %>&id=<%= prod.id %>">
			<!-- Info a mostrar -->
			<button type="text" class="flexRow pointer <% if (prod.status_registro_id==inactivo_id) { %>rojo<% } %>" id= "prod" autofocus>
				<img class="imagenChica" src="<%= prod.avatar %>" alt="<%= prod.nombre_original %>" title="<%= prod.nombre_original %>">
				<div id="infoPeli" class="flexCol">
					<p><b><%= prod.nombre_original %></b></p>
					<p class="interlineadoChico"><em><%= prod.nombre_castellano %></em></p>	
					<br>
					<p class="interlineadoChico"><%= prod.ano_estreno + " - " + prod.entidadNombre %></p>
					<% if (prod.direccion) { %>
						<p class="interlineadoChico">Direcci√≥n: <%= prod.direccion %></p>
					<% } %>
				</div>
			</button>
		</a>
	</li>
<% } %>
