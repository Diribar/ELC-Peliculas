<div id="nombres">
	<% if (codigo != "detalle") { %><a href="/producto/detalle/?entidad=<%= entidad %>&id=<%= id %><%= locals.origen && !origen.startsWith("DT") ? '&origen=' + origen : '' %>"><% } %>
		<h2>
			<%= registro.capitulo ? (registro.capitulo + ". ") : "" %>
			<%= registro.nombreCastellano ? (registro.nombreCastellano + (registro.anoEstreno ? " (" + registro.anoEstreno + ")" : "")) : '' %>
		</h2>
		<h3><em><%= registro.nombreOriginal ? registro.nombreOriginal : '' %></em></h3>
	<% if (codigo != "detalle") { %></a><% } %>
</div>
