<!-- Contenido que se desplaza -->
<nav> 
	<!-- Calificaciones -->
	<h3>Calificaciones</h3>
	<% if (registro.calificacion) { %>
		<p id="calificacionesResultados"></p>
		<div id="calificacionesGrafico"></div>
	<% } else { %>
		<p style="text-align: center; margin-top: 10px; font-style: italic;">- Sin calificaciones a√∫n -</p>
	<% } %>
	<!-- Texto -->
	<h3>Datos del Registro</h3>
	<div id="texto">
		<% for (bloque of bloqueDer) { %>
			<p <% if (bloque.titulo.startsWith("Status")) { %>class="status" id="status<%= bloque.id %>"<% } %>>
				<strong><%= bloque.titulo %>:</strong> <span><%= bloque.valor %></span>
				<% if (bloque.RCLV_entidad) { %>
					<% let datosRCLV = "entidad=" + bloque.RCLV_entidad + "&id=" + bloque.RCLV_id %>
					<a href="/rclv/detalle/?<%= datosRCLV %>">
						<i class="fa-solid fa-circle-info"></i>
					</a>
				<% } %>
			</p>
		<% } %>
	</div>
</nav>
