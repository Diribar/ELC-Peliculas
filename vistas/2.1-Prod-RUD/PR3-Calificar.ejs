<div id="tituloInfoDetalle" class="relative">
	<h3>Tu calificación</h3>
	<!-- Ayuda -->
	<div class="ayuda">
		<i class="fa-solid fa-circle-question ayudaClick desplClick">
			<ul type="dots" class="ayudaMostrar ocultar desplMostrar">
				<li></li>
			</ul>
		</i>
	</div>
</div>
<form id="calificaciones" class="marco flexColAlign">
	<% let i = 0 %>
	<% for (let atributo in atributosCalific) { %>
		<div class="flexRowAlign">
			<h4><%= atributosTitulo[i] %></h4>
			<select name="<%= atributo %>" id="">
				<% for (let opcion of atributosCalific[atributo]) { %>
					<option value="<%= opcion.valor %>"><%= opcion.nombre %></option>
				<% } %>
			</select>
		</div>
		<% i++ %>
	<% } %>
	<!-- Guardar cambios -->
	<div class="flexRowAlign">
		<!-- Guarda la calificación -->
		<button type="submit">
			<i id="guardar" class="fa-solid fa-circle-check inactivo" title='Guardá tu calificación'></i>
		</button>
		<!-- Elimina tu calificación guardada -->
		<i class="fa solid fa-circle-xmark inactivo" tabindex="-1" title='Eliminá tu calificación guardada'></i>
	</div>
</form>
