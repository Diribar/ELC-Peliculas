<tr class="edicion crud ocultar inactivo_false">
	<% let aux %>
	<!-- Url -->
	<td class="ancho_url url campo" title="<%= link.url %>">
		<div class="inputError">
			<img src="/Externa/6-LogosProvs/<%= link.prov.avatar %>" alt="<%= link.prov.nombre %>">
			<input name="url" type="text" class="input ocultar" value="<%= link.url %>">
			<div class="errores">
				<i class="fa-solid fa-circle-xmark ocultar" style="display: none"><span class="mensajeError"></span></i>
				<i class="fa-solid fa-circle-check" style="display: none"></i>
				<i class="fa-solid fa-question prov_id" style="display: none"></i>
			</div>
		</div>
	</td>
	<!-- Calidad -->
	<td class="ancho_calidad calidad campo">
		<div class="inputError">
			<% aux = link.prov.calidad %>
			<select name="calidad" class="input" <% if (aux) { %>disabled<% } %>>
				<% for (let calidad of calidades) { %>
					<option value="<%= calidad %>" <%- link.calidad==calidad ? "selected" : "" %>><%= calidad %></option>
				<% } %>
				<option value="1081" <%- link.calidad=="1081" ? "selected" : "" %>>>1080</option>
			</select>
			<%- include("CMP-Errores", {z_index: 7, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Castellano -->
	<td class="ancho_castellano castellano campo">
		<div class="inputError">
			<select name="castellano" class="input">
				<option value="1" <%- link.castellano ? "selected" : "" %>>SI</option>
				<option value="0" <%- !link.castellano ? "selected" : "" %>>NO</option>
			</select>
			<%- include("CMP-Errores", {z_index: 6, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Sub-Títulos -->
	<td class="ancho_subtitulos subtitulos campo">
		<div class="inputError">
			<select name="subtitulos" class="input">
				<option value="1" <%- link.subtitulos ? "selected" : "" %>>SI</option>
				<option value="0" <%- link.subtitulos === false ? "selected" : "" %>>NO</option>
				<option value="-" <%- link.subtitulos === null ? "selected" : "" %> class="ocultar">-</option>
			</select>
			<%- include("CMP-Errores", {z_index: 5, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Gratuito -->
	<td class="ancho_gratuito gratuito campo">
		<div class="inputError">
			<% aux = link.prov.siemprePago || link.prov.siempreGratuito %>
			<select name="gratuito" class="input" <% if (aux) { %>disabled<% } %>>
				<option value="1" <%- link.gratuito ? "selected" : "" %>>SI</option>
				<option value="0" <%- !link.gratuito ? "selected" : "" %>>NO</option>
			</select>
			<%- include("CMP-Errores", {z_index: 4, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Tipo -->
	<td class="ancho_tipo_id tipo_id campo">
		<div class="inputError">
			<% aux = !link.prov.trailer || !link.prov.pelicula %>
			<select name="tipo_id" class="input" <% if (aux) { %>disabled<% } %>>
				<option value="<%= linksTipos[0].id %>" <%- link.tipo_id==linksTipos[0].id ? "selected" : "" %>><%= linksTipos[0].nombre %></option>
				<% if (entidad != "colecciones") { %>
					<option value="<%= linksTipos[1].id %>" <%- link.tipo_id==linksTipos[1].id ? "selected" : "" %>><%= linksTipos[1].nombre %></option>
				<% } %>
			</select>
			<%- include("CMP-Errores", {z_index: 3, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Completo -->
	<td class="ancho_completo completo campo">
		<div class="inputError">
			<% aux = link.prov.siempreCompleta %>
			<select name="completo" class="input" <% if (aux) { %>disabled<% } %>>
				<option value="1" <%- link.completo ? "selected" : "" %>>SI</option>
				<option value="0" <%- !link.completo ? "selected" : "" %>>NO</option>
			</select>
			<%- include("CMP-Errores", {z_index: 2, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Parte -->
	<td class="ancho_parte parte campo">
		<div class="inputError">
			<% aux = link.prov.siempreCompleta %>
			<input name="parte" type="text" class="input" value="<%= link.parte %>" <% if (aux) { %>disabled<% } %>>
			<%- include("CMP-Errores", {z_index: 1, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Status -->
	<td class="ancho_status status taparMotivo"><%= link.statusRegistro.nombre %></td>
	<!-- Propio -->
	<td class="ancho_propio taparMotivo"><%= link.cond.propios ? "SI" : link.cond.ajenos ? "NO" : "-" %></td>
	<!-- Acción -->
	<td class="ancho_accion accion">
		<button type="button" class="inactivo" title='Guardar'>
			<i class="fa-solid fa-floppy-disk"></i>
		</button>
	</td>
</tr>
