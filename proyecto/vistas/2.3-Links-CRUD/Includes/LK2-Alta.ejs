<tr class="alta inactivo_false">
	<form action="">
	<!-- Url y Proveedor_id-->
	<td class="ancho_url url campo">
		<div class="inputError">
			<input name="url" type="text" autofocus class="input" autocomplete="off">
			<!-- Errores -->
			<div class="errores">
				<i class="fa-solid fa-circle-xmark ocultar" style="z-index: 8"><span class="mensajeError"></span></i>
				<i class="fa-solid fa-circle-check prov_id"></i>
				<i class="fa-solid fa-question prov_id"></i>
			</div>
		</div>
	</td>
	<!-- Calidad -->
	<td class="ancho_calidad calidad desperdicio campo">
		<div class="inputError">
			<select name="calidad" class="input ocultar">
				<option value="" selected>?</option>
				<% for (let calidad of calidades) { %>
					<option value="<%= calidad %>"><%= calidad %></option>
				<% } %>
				<option value="1081">Sup.</option>
			</select>
			<%- include("CMP-Errores", {z_index: 7, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Castellano -->
	<td class="ancho_castellano castellano desperdicio campo">
		<div class="inputError">
			<select name="castellano" class="input ocultar">
				<option value="" selected>?</option>
				<option value="1">SI</option>
				<option value="0">NO</option>
			</select>
			<%- include("CMP-Errores", {z_index: 6, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Sub-Títulos -->
	<td class="ancho_subtitulos subtitulos desperdicio campo">
		<div class="inputError">
			<select name="subtitulos" class="input ocultar">
				<option value="" selected>?</option>
				<option value="1">SI</option>
				<option value="0">NO</option>
				<option value="-" class="ocultar">-</option>
			</select>
			<%- include("CMP-Errores", {z_index: 5, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Gratuito -->
	<td class="ancho_gratuito gratuito desperdicio campo">
		<div class="inputError">
			<select name="gratuito" class="input ocultar">
				<option value="" selected>?</option>
				<option value="1">SI</option>
				<option value="0">NO</option>
			</select>
			<%- include("CMP-Errores", {z_index: 4, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Tipo -->
	<td class="ancho_tipo_id tipo_id desperdicio campo">
		<div class="inputError">
			<select name="tipo_id" class="input ocultar">
				<option value="">Elejí</option>
				<option value="<%= linksTipos[0].id %>" <%- entidad == "colecciones" ? "selected" : "" %>><%= linksTipos[0].nombre %></option>
				<% if (entidad != "colecciones") { %>
					<option value="<%= linksTipos[1].id %>"><%= linksTipos[1].nombre %></option>
				<% } %>
			</select>
			<%- include("CMP-Errores", {z_index: 3, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Completo -->
	<td class="ancho_completo completo desperdicio campo">
		<div class="inputError">
			<select name="completo" class="input ocultar">
				<option value="" selected>Elejí</option>
				<option value="1">SI</option>
				<option value="0">NO</option>
			</select>
			<%- include("CMP-Errores", {z_index: 2, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Parte -->
	<td class="ancho_parte parte desperdicio campo">
		<div class="inputError">
			<input name="parte" type="text" class="input ocultar">
			<%- include("CMP-Errores", {z_index: 1, ocultarOK: true}) %>
		</div>
	</td>
	<!-- Status -->
	<td class="ancho_status status desperdicio">
	</td>
	<!-- Propio -->
	<td class="ancho_propio propio desperdicio">
	</td>
	<!-- Acción -->
	<td class="ancho_accion accion">
		<button type="button" class="inactivo" title='Guardar' tabindex="-1">
			<i class="fa-solid fa-floppy-disk"></i>
		</button>
	</td>
</tr>
