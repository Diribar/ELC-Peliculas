<div id="<%= locals.nombre %>" class="errores" >

	<!-- Errores -->
	<i
		class="<%= iconos.faSolid %> <%= iconos.xMark %><%- !locals.errores || locals.nombre && !errores[nombre] ? ' ocultar' : '' %>"
		<% if (locals.z_index) { %>style="z-index: <%= z_index %> "<% } %>
	>
		<span class="mensajeError"><%- locals.nombre && locals.errores && errores[nombre] ? errores[nombre] : "" %></span>
	</i>

	<% if (tema == "prodRud" && codigo == "edicion") { %>
		<!-- Advertencia -->
		<i class="<%= iconos.faSolid %> fa-triangle-exclamation ocultar">
			<span class="mensajeError"></span>
		</i>
	<% } %>

	<!-- OK -->
	<i
		class="<%= iconos.faSolid %> <%= iconos.check %><%- (locals.nombre && (locals.dataEntry && !dataEntry[nombre] || locals.errores && errores[nombre])) ? ' ocultar' : '' %>"
		<% if (locals.ocultarOK) { %>style="display: none;"<% } %>
	>
	</i>
</div>
