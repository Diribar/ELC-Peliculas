<!DOCTYPE html>
<html lang="es">
<head>
	<% let tema = locals.tema %>
	<!-- Head genérico -->
	<%- include('CMP-1Head') %>
	<!-- Heads personalizados -->
	<% if (tema == "usuario") { %><%- include("US-1Head") %><% } %>
	<% if (tema == "prodAgregar") { %><%- include("PA-1Head") %><% } %>
	<% if (tema == "prodRud") { %><%- include("PR-1Head") %><% } %>
	<% if (tema == "rclvCrud") { %><%- include("RCLV-1Head") %><% } %>
	<% if (tema == "linksCRUD") { %><%- include("LK-1Head") %><% } %>
	<% if (tema == "responsabilidad" || tema == "fmCrud") { %><%- include("FM-1Head") %><% } %>
	<% if (tema == "revisionEnts") { %><%- include("RE-1Head") %><% } %>
	<% if (tema == "revisionUs") { %><%- include("RU-1Head") %><% } %>
	<% if (tema == "mantenimiento") { %><%- include("MT-1Head") %><% } %>
	<% if (tema == "consultas") { %><%- include("CN-1Head") %><% } %>
	<% if (tema == "institucional") { %><%- include("IN-1Head") %><% } %>
	<% if (tema == "gráficos") { %><%- include("GR-1Head") %><% } %>
</head>
<body>
	<!-- Header genérico -->
	<%- include('CMP-0Header') %>
	<main>
		<!-- Si cualquiera es falso, se muestra -->
		<% if (!estructuraPersonalizada) { %>
			<!-- CuerpoFooter e Imagen Derecha -->
			<section id="cuerpoFooterImgDer">
				<!-- Cuerpo y Footer -->
				<div id="cuerpoFooter">
					<!-- Cuerpo -->
					<div id="cuerpo">
						<% if (false) { %>
						<% } else if (tema == "usuario") { %><%- include("US-2Cuerpo") %>
						<% } else if (tema == "fmCrud") { %><%- include("FM-2Cuerpo") %>
						<% } else if (tema == "prodAgregar") { %><%- include("PA-2Cuerpo") %>
						<% } else if (tema == "prodRud") { %><%- include("PR-2Cuerpo") %>
						<% } else if (tema == "rclvCrud") { %><%- include("RCLV-2Cuerpo") %>
						<% } else if (tema == "linksCRUD") { %><%- include("LK-2Cuerpo") %>
						<% } else if (tema == "revisionEnts") { %><%- include("RE-2Cuerpo") %>
						<% } else if (tema == "revisionUs") { %><%- include("RU-2Cuerpo") %>
						<% } else if (tema == "mantenimiento") { %><%- include("MT-2Cuerpo") %>
						<% } else if (tema == "consultas") { %><%- include("CN-2Cuerpo") %>
						<% } else if (tema == "institucional") { %><%- include("IN-2Cuerpo") %>
						<% } else if (tema == "responsabilidad") { %><%- include(locals.vista) %>
						<% } else if (tema == "gráficos") { %><%- include("GR-2Cuerpo") %>
						<% } %>
					</div>
					<% if (!locals.omitirFooter) { %><%- include('CMP-Footer') %><% } %>
				</div>
				<% if (!locals.omitirImagenDerecha) { %><%- include('CMP-ImagenDerecha') %><% } %>
			</section>
		<% } else { %>
			<% if (false) { %>
			<% } else if (tema == "prodRud") { %><%- include("PR-2Cuerpo") %>
			<% } else if (tema == "rclvCrud") { %><%- include("RCLV-2Cuerpo") %>
			<% } else if (tema == "revisionEnts") { %><%- include("RE-2Cuerpo") %>
			<% } %>
		<% } %>
		<%- include('CMP-CartelGenerico') %>
	</main>
</body>
</html>