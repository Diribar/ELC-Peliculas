<% if (tema == "responsabilidad") { %>
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM9-Responsab.css">

<% } else if (["fmCrud", "correccion"].includes(tema)) { %>
	<!-- fmCrud: Inactivar, Recuperar, Rechazar, Eliminar -->

	<!-- Compartido -->
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-1-Encabezado.css">
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-Detalle.css">
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-0MediaQuery.css">
	<link rel="stylesheet" href="/publico/formatos/0-Compartido/Main/MN-Iconos.css">
	<% if (tema == "fmCrud") { %>
		<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-PR-DatosLargos.css">
		<link rel="stylesheet" href="/publico/formatos/0-Compartido/Main/MN-DatosBreves.css">
	<% } %>
	<% if (tema == "correccion") { %>
		<link rel="stylesheet" href="/publico/formatos/9-Miscelaneas/Corregir.css">
	<% } %>

	<% if (codigo == "inactivar") { %>
		<!-- Inactivar -->
		<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-PR-ElegirMotivo.css">
		<script src="/publico/javascript/2.0-Familias/FM-PR-ElegirMotivo.js"></script>
	<% } %>
	<% if (["recuperar", "eliminar"].includes(codigo)) { %>
		<!-- Recuperar, Eliminar -->
		<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-PR-HistorialStatus.css">
		<% if (codigo == "recuperar") { %>
			<script src="/publico/javascript/2.0-Familias/FM-PR-Recuperar.js"></script>
		<% } %>
	<% } %>
<% } %>
