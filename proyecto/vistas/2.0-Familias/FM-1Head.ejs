	<!-- Compartido -->
	<link rel="stylesheet" href="/publico/formatos/0-Compartido/Main/MN-DatosBreves.css">
	<link rel="stylesheet" href="/publico/formatos/0-Compartido/Main/MN-Iconos.css">
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-Encabezado.css">
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-Detalle.css">
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM-0MediaQuery.css">
	<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM8-DatosLargos.css">

	<% if (["inactivar", "rechazar"].includes(codigo)) { %>
		<!-- Inactivar, Rechazar -->
		<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM8-InactivarRech.css">
	<% } else { %>
		<!-- Recuperar, Eliminar -->
		<link rel="stylesheet" href="/publico/formatos/2.0-Familias/FM8-HistorialStatus.css">
