		<ul type="none" id="listado">
			<%- include("PA2-DS1-ProdsNuevos") %>
			<%- include("PA2-DS2-IngrManual") %>
			<%- include("PA2-DS3-ProdsYaEnBD") %>
		</ul>
		<i class="fa-solid fa-caret-right mover inactivo"></i>
	</div>
	<div id="sectorIconos">
		<div class="iconos">
			<a href="/producto/agregar/palabras-clave" title="Volver al paso anterior"><i class="fa-solid <%= iconos.izquierda %>"></i></a>
		</div>
	</div>
</div>