		<p id="mensaje"></p>
		<p><u>Palabra/s clave</u>: <b><%= palabrasClave %></b></p>
	</div>
	<div id="cuadroDeResultados">
		<%- include('MN-CartelProgreso', {tarea: "Buscando las películas"}) %>
