<div id="tituloPrincipal">
	<h1 id="titulo">Agregá una Película o Colección a nuestra Base de Datos</h1>
</div>
<div id="dataEntry">
	<div id="encabezado">
		<div id="tituloSecundario">
			<h2>Paso 2 de 5: Desambiguar - Elegí tu opción</h2>
			<%- include("MN-Ayuda", {mensajes: ["<u>Fondo verde</u>: películas que no tenemos en nuestra BD.", "<u>Fondo azul</u>: películas que ya tenemos en nuestra BD.", "Están ordenadas desde la más reciente a las más antigua."]}) %>
