<% if (tema == "prodRud" && codigo == "detalle") { %>
	<a href="<%= '/producto/calificar/?entidad=' + entidad + '&id=' + id %>">
		<i class="fa-solid fa-chart-simple fa-rotate-90 <%= locals.yaCalificada || !!locals.califUsuario %>" tabindex="-1"></i>
	</a>
<% } %>
<% if (codigo == "calificar") { %>
	<a href="<%= '/producto/detalle/?entidad=' + entidad + '&id=' + id %>">
		<i class="fa-solid fa-circle-info" tabindex="-1"></i>
	</a>
<% } %>
