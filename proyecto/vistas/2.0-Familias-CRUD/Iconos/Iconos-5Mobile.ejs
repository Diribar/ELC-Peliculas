<div id="mobile" class="iconos">
	<% if (codigo == "detalle") { %>
		<i id="muestraDL" class="fa-solid fa-circle-info" tabindex="-1"></i>
		<i id="muestraDB" class="fa-solid fa-chart-line" tabindex="-1"></i>
	<% } %>
	<% if (codigo == "calificar") { %>
		<i id="muestraCL" class="fa-solid fa-chart-simple fa-rotate-90 <%= locals.yaCalificada || !!locals.califUsuario %>" tabindex="-1"></i>
		<i id="muestraDB" class="fa-solid fa-chart-line" tabindex="-1"></i>
	<% } %>
	<%- include("Iconos-5MobileHref") %>
</div>
<% if (locals.interesDelUsuario) { %>
	<div class="iconos">
		<%- include("Iconos-5MobileHref") %>
		<i id="ppp" class="<%= interesDelUsuario.icono %>" tabindex="-1" title="<%= interesDelUsuario.nombre %>"></i>
	</div>
<% } %>
