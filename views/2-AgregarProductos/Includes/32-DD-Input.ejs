<% if ((dataEntry.rubroAPI=="movie" && datos[i].peli) || (dataEntry.rubroAPI!="movie" && datos[i].colec)) { %>
	<section class="label-input flex-row">
		<label><%= datos[i].titulo %></label>
		<!-- Data-entry y Errores -->
		<div class="input-error">
			<input type="text" class="input" name="<%= datos[i].campo %>"
				value="<%= dataEntry[datos[i].campo] %>"
				<% if (datos[i].id) { %>id="numero" placeholder="sólo números"<% } %>
				<% if (dataEntry[datos[i].campo] && !errores[datos[i].campo]) { %>disabled<% } %> 
			>
			<%- (dataEntry[datos[i].campo] && !errores[datos[i].campo]) ? include("39-DD-Mensajes") : "" %>
			<!-- Errores -->
			<i class="fas fa-times-circle<%- !errores[datos[i].campo] ? ' ocultar' : '' %>">
				<span class="mensajeError"><%= errores[datos[i].campo] %></span>
			</i>
		</div>
	</section>
<% } %> 
