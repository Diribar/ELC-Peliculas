<tr class="yaExistentes">
	<!-- Url -->
	<td class="logoProv" title="<%= link.url %>">
		<a href="//<%= link.url %>" target="_blank">
			<img src="/imagenes/0-Logos/<%= link.link_prov.avatar %>" alt="<%= link.link_prov.nombre %>">
		</a>
		<div class="ocultar" id="link_id"><%= link.id %></div>
	</td>
	<!-- Calidad -->
	<td><%= link.calidad %></td>
	<!-- Tipo -->
	<td><%= link.link_tipo.nombre %></td>
	<!-- Completo -->
	<td><%- link.completo ? "SI" : "NO" %></td>
	<!-- Parte -->
	<td class="<%- link.completo ? 'desperdicio' : '' %>">
		<%- !link.completo ? link.parte : '' %>
	</td>
	<!-- Gratuito -->
	<td><%= link.gratuito ? "SI" :  "NO" %></td>
	<!-- Status -->
	<td><%= link.status_registro.links %> </td>
	<!-- AcciÃ³n -->
	<td>
		<% if (link.creado_por_id == usuario.id) { %>
			<i class="fa-solid fa-trash-can out" title='Eliminar'></i>
		<% } else { %>
			<i class="fa-solid fa-circle-xmark out" title='Inactivar'></i>
		<% } %>
	</td>
</tr>
