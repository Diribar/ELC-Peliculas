<!DOCTYPE html>
<html lang="es">
<head>
	<%- include('1-Head') %>
	<%- include('0-Revisar-Head') %>
</head>
<body>
	<%- include('21-Header') %>
	<main id="cuerpo-y-footer">
		<%- include('23-Footer') %>
		<section id="cuerpo">
			<%- include('Timer') %>
			<h1 id="titulo"><%= titulo %></h1>
			<div id="imagenes">
				<!-- Versión editada -->
				<div id="editada" class="flex-col">
					<!-- Liberar y salir -->
					<i class="fa-solid fa-circle-left" tabindex="-1" title="Salir sin cambios"></i>
					<!-- Imagen -->
					<h2>Imagen de Edición</h2>
					<img src="<%= avatar.edicion %>" title="Realizar el reemplazo">
				</div>
				<!-- Versión original -->
				<div id="original" class="flex-col">
					<h2>Imagen Original</h2>
					<img src="<%= avatar.original %>">
				</div>
			</div>
		</section>
	</main>
</body>
</html>