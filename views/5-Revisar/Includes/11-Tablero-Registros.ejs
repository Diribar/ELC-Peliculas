<% let regs = registros.filter((n) => n.status_registro_id == estado) %>
<% if (regs.length) { %>
	<div class="contenido">
		<h3><%= titulo %> </h3>
		<% for (let registro of regs) { %>
			<p>
				<a href="/revision/redireccionar/?entidad=<%= registro.entidad %>&id=<%= registro.id %>">
					<%= registro.abrev %> - <%= registro.nombre %>
				</a>
			</p>
		<% } %>	
	</div>
<% } %>